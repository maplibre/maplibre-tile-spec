{
  "name": "@maplibre/mlt",
  "version": "0.0.1-alpha.7",
  "main": "dist/mlt.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "license": "(MIT OR Apache-2.0)",
  "homepage": "https://github.com/maplibre/maplibre-tile-spec/#readme",
  "keywords": [
    "maplibre",
    "gis",
    "vector"
  ],
  "scripts": {
    "prepare": "npm run generate",
    "build": "tsc",
    "test": "jest",
    "serve": "http-server .",
    "serve:data": "http-server ../test/data --cors",
    "serve:data2": "node test/server/dataServer.js ../../../test/data",
    "serve:gzip": "http-server ../test/data -g --cors",
    "lint": "eslint",
    "format": "prettier --write \"**/*.ts\"",
    "generate": "buf generate ../spec/schema/mlt_tileset_metadata.proto && node -e \"const fs=require('fs'); const path=require('path'); const dir=path.join('src','metadata','tileset'); fs.renameSync(path.join(dir,'mlt_tileset_metadata_pb.ts'), path.join(dir,'tilesetMetadata.g.ts'))\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maplibre/maplibre-tile-spec.git"
  },
  "bugs": {
    "url": "https://github.com/maplibre/maplibre-tile-spec/issues"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.58.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@mapbox/vector-tile": "^2.0.4",
    "@maplibre/maplibre-gl-style-spec": "^24.3.0",
    "@types/benchmark": "^2.1.5",
    "@types/bytebuffer": "^5.0.49",
    "@types/earcut": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/varint": "^6.0.3",
    "benchmark": "^2.1.4",
    "body-parser": "^2.2.0",
    "bytebuffer": "^5.0.1",
    "codegen": "^0.1.0",
    "cors": "^2.8.5",
    "earcut": "^3.0.2",
    "eslint": "^9.37.0",
    "express": "^5.1.0",
    "fflate": "^0.8.2",
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "nyc": "^17.1.0",
    "pbf": "^4.0.1",
    "prettier": "^3.6.2",
    "semver": "^7.7.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "varint": "^6.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "bitset": "^5.2.3"
  }
}
